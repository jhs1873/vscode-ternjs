{
  "name": "ternjs",
  "displayName": "Ternjs",
  "description": "Ternjs for Visual Studio Code",
  "version": "0.0.2",
  "publisher": "hsiaosiyuan0",
  "icon": "ternjs_icon.png",
  "engines": {
    "vscode": "^1.16.0"
  },
  "categories": [
    "Languages"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "ternjs.generateConfig",
        "title": "Ternjs: Generate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "watch": "tsc -watch -p .",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^2.5.2",
    "vscode": "^1.1.5",
    "mocha": "^3.5.0",
    "@types/node": "^7.0.43",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "vscode-languageclient": "^3.4.2"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/hsiaosiyuan0/vscode-ternjs.git"
  },
  "keywords": [
    "vscode",
    "ternjs"
  ],
  "author": "hsiaosiyuan0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hsiaosiyuan0/vscode-ternjs/issues"
  },
  "homepage": "https://github.com/hsiaosiyuan0/vscode-ternjs#readme"
}
